{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "allowJs":true,
    "strict": true,
    "jsx": "preserve",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "baseUrl": ".",
    "paths":{
      "@/*":["src/*"]
    }
  },
  "include": [ 
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "src/**/*.jsx" 
  ],
  "exclude": ["node_modules"]
}